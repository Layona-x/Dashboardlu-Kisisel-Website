
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
 
    <script src="https://cdn.tailwindcss.com"></script>
  </head>
  <body>
    <%- include('./includes/navbar.ejs') -%>
    <%animes.forEach(animes => {%>
    <div class="card ml-16 w-96 md:ml-32 w-3/4">
  <img src="/uploads/<%= animes.photo %> " class="card-img-top h-48" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= animes.title %></h5>
    <p class="card-text"><%= animes.description %></p>
      <hr>
      <br>
    <a href="<%= animes.link %>" class="btn btn-primary"><%= animes.title %></a>
  </div>
    </div>
    <% }) %>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
  </body>
