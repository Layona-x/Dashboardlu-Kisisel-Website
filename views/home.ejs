<% let discordUser = {
    username: user.data.discord_user.username,
    public_flags: user.data.discord_user.public_flags,
    id: user.data.discord_user.id,//number
    tag: user.data.discord_user.discriminator,//number
    avatar: user.data.discord_user.avatar,
    avatarURL: `https://cdn.discordapp.com/avatars/${user.data.discord_user.id}/${user.data.discord_user.avatar}.webp`,
    status: user.data.discord_status
  }
   %>
<head>
  <title><% discordUser.username %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="./home.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">   
    <link href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css" rel="stylesheet">

    </head>
  <body>
  <%- include('./includes/navbar.ejs') -%>
    <div class="ml-40 text-primary">
    <img class=" md:ml-56 rounded-full border-[3.2px] w-[8rem] h-[8rem] border-[#<%= discordUser.status === 'dnd' ? 'ED4245' :  discordUser.status === 'idle' ? 'faa61a' : discordUser.status === 'online' ? '57F287' : discordUser.status === 'offline' ? '99aab5' : 'EB459E' %>]" src="<%= discordUser.avatarURL %>"/>
     <h1 class="md:pl-52"><%= discordUser.username %></h1>
      
    </div>
  <%- include('./includes/footer.ejs') -%>
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
    
   
  </body>
    
    
    
    
    
    
    
    
    
    